<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Query all enabled replication agents"
    jcr:primaryType="sling:OrderedFolder"
    sling:resourceType="slingPipes/xpath"
    expr="/jcr:root/etc/replication//*[jcr:like(@sling:resourceType, &quot;cq/replication/components/%agent&quot;) and enabled=true]"/>
